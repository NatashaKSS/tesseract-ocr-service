    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "10.x"
  },
  "keywords": []
