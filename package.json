{
  "name": "tesseract-ocr-service",
  "description": "A NodeJS web service that performs OCR on an image hosted at a URL",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start-prod",
    "build": "babel src -d dist",
    "start-prod": "node dist/index.js",
    "start-dev": "nodemon --inspect ./src --exec babel-node src/index.js",
    "test": "mocha --require babel-core/register './src/**/test/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatashaKSS/node-express-barebones.git"
  },
