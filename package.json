// set local ./node_modules/... ref to babel
{
  "name": "tesseract-ocr-service",
  "description": "A NodeJS web service that performs OCR on an image hosted at a URL",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start-prod",
    "build": "babel src --out-dir dist --ignore src/lib --copy-files src/lib",
    "start-prod": "node dist/index.js",
    "start-dev": "nodemon --exec babel-node src/index.js",
    "test": "mocha --require babel-core/register './src/**/test/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatashaKSS/node-express-barebones.git"
  },
  "author": "Natasha Koh Sze Sze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NatashaKSS/node-express-barebones/issues"
  },
  "homepage": "https://github.com/NatashaKSS/node-express-barebones#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "lodash": "^4.17.10",
    "path": "^0.12.7",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "tesseract.js": "^1.0.10",
    "tesseract.js-core": "^1.0.2",
    "uuid": "^3.3.2",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "8.x"
  },
  "keywords": []
}
