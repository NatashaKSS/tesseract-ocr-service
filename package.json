  "bugs": {
    "url": "https://github.com/NatashaKSS/node-express-barebones/issues"
  },
  "homepage": "https://github.com/NatashaKSS/node-express-barebones#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "fs-extra": "^5.0.0",
    "path": "^0.12.7",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "tesseract-lambda": "^0.0.1",
    "tesseract.js": "^1.0.10",
    "tesseract.js-core": "^1.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
